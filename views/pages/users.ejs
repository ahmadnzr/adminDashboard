<div class="info info-user table-responsive">
  <table id="example" class="table table-hover" style="width: 100%">
    <thead>
      <tr>
        <th>No</th>
        <th>Name</th>
        <th>Position</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% data.map((item) => { %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.position %></td>
        <td>
          <a href="/users/<%= item.id %> ">view details</a>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
